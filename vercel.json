{
  "version": 2,
  "public": true,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.niklinks.vercel.app"
        }
      ],
      "destination": "https://niklinks.vercel.app/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(png|ico|jpg|jpeg|webp|svg))",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}
